{
  "name": "reactfree-jsx",
  "version": "1.12.0",
  "author": "Melvin Doucet",
  "description": "A package to use JSX without React",
  "keywords": [
    "vanilla",
    "js",
    "jsx",
    "tsx",
    "vite"
  ],
  "type": "module",
  "main": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "default": "./dist/mod.js"
    },
    "./extra": {
      "types": "./dist/extra/mod.d.ts",
      "default": "./dist/extra/mod.js"
    },
    "./extra/router": {
      "types": "./dist/extra/router/mod.d.ts",
      "default": "./dist/extra/router/mod.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime/mod.d.ts",
      "default": "./dist/jsx-dev-runtime/mod.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/mod.d.ts",
      "default": "./dist/jsx-runtime/mod.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MelvDouc/reactfree-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/MelvDouc/reactfree-jsx/issues",
    "email": "melv.douc@gmail.com"
  },
  "homepage": "https://github.com/MelvDouc/reactfree-jsx#readme",
  "devDependencies": {
    "jsdom": "^26.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}